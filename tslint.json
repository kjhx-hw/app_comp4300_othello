{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        140
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "object-literal-shorthand": false,
    "no-console": true,
    "curly": [
      "error",
      "multi",
      "consistent"
    ],
    "eqeqeq": [
      "error",
      "smart"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        140
      ]
    }
  }
}